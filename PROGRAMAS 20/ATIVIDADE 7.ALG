Algoritmo "Atividade 7"

Var
sal,per,nsal,msal:real

Inicio
      escreval ("informe o seu salario: ")
leia (sal)
      ESCREVAL ("informe o persentual de almento: ")
leia (per)
  nsal <- sal+(sal*per/100)
  msal <- nsal-sal
escreval ("o valor do seu almento é ",msal)
escreval ("o seu salario atual é igual a ",nsal)

Fimalgoritmo

